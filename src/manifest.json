{
    "manifest_version": 2,
    "name": "Express Debug",
    "version": "1.0",
    "minimum_chrome_version": "10.0",
    "devtools_page": "loader.html",
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*",
        "webRequest",
        "webRequestBlocking",
        "storage"
    ],
    "browser_style": true,
    "background": {
        "scripts": [
            "background.js"
        ]
    }
  }
  